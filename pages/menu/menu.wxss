/* pages/menu/menu.wxss */
.menu_main{
  background-color: #fafafa;
  height: 100vh;
  color: #888;
  font-size: 13px;
}
.menu_title{
  padding: 15px 15px 10px;
}
.menu_inner{
  
  margin: 0 14px;
}
.menu_item{
  display: inline-block;
  width: 33%;
  text-align: center;
  padding: 20px 0;
  background-color: #fff;
  border: .3px solid #fafafa;
}
.menu_item image{
  width: 50px;
  height: 50px;
}
.menu_item_summary{
  height: 17px;
}